{
  "name": "one-zero",
  "version": "1.1.470",
  "private": true,
  "sideEffects": false,
  "files": [],
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build:icon": "tauri icon ./src-tauri/icons/icon.png",
    "build:tauri": "tauri build --debug",
    "build:web": "one build",
    "data:reset": "docker compose down --volumes; rm /tmp/onezero_replica.db",
    "dev": "one dev",
    "dev:clean": "one dev --clean",
    "dev:tauri": "tauri dev",
    "dev:ios": "one run:ios",
    "dev:android": "one run:android",
    "docker": "docker compose --env-file .env",
    "docker:start:clean": "yarn data:reset; yarn docker up",
    "migrate:auth": "npx @better-auth/cli migrate --config ./src/better-auth/auth.ts",
    "seed": "tsx ./src/postgres/seed.ts",
    "serve": "one serve",
    "zero": "yarn zero:build-schema && zero-cache --schema-file ./src/zero/zero-schema.json",
    "zero:build-schema": "cd ./src/zero && zero-build-schema -p './schema.ts'"
  },
  "dependencies": {
    "@rocicorp/zero": "^0.12.2025012501",
    "@tamagui/config": "^1.125.24",
    "@tamagui/linear-gradient": "^1.125.24",
    "@tamagui/lucide-icons": "^1.125.24",
    "@tamagui/themes": "^1.125.24",
    "@tamagui/toast": "^1.125.24",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-deep-link": "^2.0.1",
    "@tauri-apps/plugin-fs": "^2.2.0",
    "@tauri-apps/plugin-shell": "^2",
    "@vxrn/better-auth": "workspace:*",
    "@vxrn/color-scheme": "workspace:*",
    "@vxrn/emitter": "workspace:*",
    "better-auth": "^1.1.15",
    "expo": "~52.0.17",
    "one": "workspace:*",
    "pg": "^8.13.1",
    "react": "^19.0.0",
    "react-native": "^0.76.5",
    "react-native-reanimated": "~3.16.5",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "^0.19.12",
    "tamagui": "^1.125.24"
  },
  "devDependencies": {
    "@react-native-community/cli": "15.1.3",
    "@tamagui/vite-plugin": "^1.125.24",
    "@tauri-apps/cli": "^2",
    "@types/node": "^22.1.0",
    "@types/pg": "^8.11.11",
    "tsx": "^4.19.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
