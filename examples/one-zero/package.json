{
  "name": "one-zero",
  "version": "1.1.512",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "files": [],
  "scripts": {
    "build:icon": "tauri icon ./src-tauri/icons/icon.png",
    "build:tauri": "tauri build --debug",
    "build:web": "one build",
    "data:reset": "docker compose down --volumes; rm /tmp/onezero_replica.db",
    "dev": "one dev",
    "dev:android": "one run:android",
    "dev:clean": "one dev --clean",
    "dev:ios": "one run:ios",
    "dev:tauri": "tauri dev",
    "docker": "docker compose --env-file .env",
    "docker:start:clean": "yarn data:reset; yarn docker up",
    "migrate:auth": "npx @better-auth/cli migrate --config ./src/better-auth/auth.ts",
    "seed": "tsx ./src/postgres/seed.ts",
    "serve": "one serve",
    "typecheck": "tsc --noEmit",
    "zero": "zero-cache-dev -p ./src/zero/schema.ts"
  },
  "dependencies": {
    "@rocicorp/zero": "^0.20.2025052100",
    "@tamagui/config": "^1.132.9",
    "@tamagui/linear-gradient": "^1.132.9",
    "@tamagui/lucide-icons": "^1.132.9",
    "@tamagui/themes": "^1.132.9",
    "@tamagui/toast": "^1.132.9",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-deep-link": "^2.0.1",
    "@tauri-apps/plugin-fs": "^2.2.0",
    "@tauri-apps/plugin-shell": "^2",
    "@vxrn/better-auth": "workspace:*",
    "@vxrn/color-scheme": "workspace:*",
    "@vxrn/emitter": "workspace:*",
    "better-auth": "^1.2.8",
    "expo": "53.0.20",
    "one": "workspace:*",
    "pg": "^8.13.1",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "react-native": "~0.79.2",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "tamagui": "^1.132.9"
  },
  "devDependencies": {
    "@react-native-community/cli": "19.0.0",
    "@tamagui/vite-plugin": "^1.132.9",
    "@tauri-apps/cli": "^2",
    "@types/node": "^22.1.0",
    "@types/pg": "^8.11.11",
    "tsx": "^4.19.0",
    "typescript": "^5.7.3",
    "vite": "^7.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
