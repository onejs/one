{
  "name": "one-zero",
  "version": "1.1.371",
  "private": true,
  "sideEffects": false,
  "files": [],
  "type": "module",
  "scripts": {
    "build:icon": "tauri icon ./src-tauri/icons/icon.png",
    "build:tauri": "tauri build --debug",
    "build:web": "one build",
    "data:reset": "docker compose down --volumes; rm /tmp/onezero_replica.db",
    "dev": "one dev",
    "dev:clean": "one dev --clean",
    "dev:tauri": "tauri dev",
    "dev:ios": "one run:ios",
    "dev:android": "one run:android",
    "docker": "docker compose --env-file .env",
    "docker:start:clean": "yarn data:reset; yarn docker up",
    "migrate:auth": "npx @better-auth/cli migrate --config ./src/better-auth/auth.ts",
    "seed": "tsx ./src/postgres/seed.ts",
    "serve": "one serve",
    "zero": "yarn zero:build-schema && zero-cache --schema-file ./src/zero/zero-schema.json",
    "zero:build-schema": "cd ./src/zero && zero-build-schema -p './schema.ts'"
  },
  "dependencies": {
    "@rocicorp/zero": "0.9.2024121900",
    "@tamagui/config": "^1.120.2",
    "@tamagui/linear-gradient": "^1.120.2",
    "@tamagui/lucide-icons": "^1.120.2",
    "@tamagui/themes": "^1.120.2",
    "@tamagui/toast": "^1.120.2",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-deep-link": "^2.0.1",
    "@tauri-apps/plugin-fs": "^2.2.0",
    "@tauri-apps/plugin-shell": "^2",
    "@vxrn/better-auth": "1.1.371",
    "@vxrn/color-scheme": "1.1.371",
    "@vxrn/emitter": "1.1.371",
    "better-auth": "1.0.23-beta.3",
    "expo": "~51.0.28",
    "one": "1.1.371",
    "pg": "^8.13.1",
    "react": "^18.3.1",
    "react-native": "0.74.5",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "^0.19.12",
    "tamagui": "^1.120.2"
  },
  "devDependencies": {
    "@tamagui/vite-plugin": "^1.120.2",
    "@tauri-apps/cli": "^2",
    "@types/node": "^22.1.0",
    "tsx": "^4.19.0",
    "vite": "^6.0.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
