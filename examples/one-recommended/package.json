{
  "name": "example-recommended",
  "version": "1.1.511",
  "private": true,
  "type": "module",
  "scripts": {
    "android": "one run:android",
    "build:web": "one build",
    "clean": "one clean",
    "db:check": "bash -c \"$npm_execpath db:push 2>&1 >/dev/null | grep ECONNREFUSED >/dev/null; if [ $? -eq 1 ]; then echo '\n\n‚ùå Note! Error running db:push, you may need to set up and run your db. See README.md\n\n'; exit 1; fi\"",
    "db:generate": "drizzle-kit generate --config ./code/db/drizzle.config.ts",
    "db:init": "$npm_execpath db:generate && $npm_execpath db:push && $npm_execpath db:seed",
    "db:migrate": "dotenvx run -f .env -- tsx ./code/db/run-migrations.ts",
    "db:push": "drizzle-kit push --config ./code/db/drizzle.config.ts",
    "db:seed": "dotenvx run -f .env -- tsx ./code/db/seed.ts",
    "dev": "$npm_execpath db:check && $npm_execpath one dev",
    "dev:clean": "one dev --clean",
    "ios": "one run:ios",
    "prebuild:native": "one prebuild",
    "serve": "one serve",
    "typecheck": "tsc --noEmit",
    "upgrade:tamagui": "$npm_execpath up '*tamagui*' '@tamagui/*'"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.12.1",
    "@tamagui/config": "^1.132.9",
    "@tamagui/image-next": "^1.132.9",
    "@tamagui/lucide-icons": "^1.132.9",
    "@vxrn/color-scheme": "workspace:*",
    "better-sqlite3": "^11.2.1",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.24.2",
    "drizzle-orm": "^0.33.0",
    "expo": "53.0.20",
    "expo-modules-core": "~2.5.0",
    "one": "workspace:*",
    "postgres": "^3.4.4",
    "react": "~19.0.0",
    "react-native": "~0.79.2",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "tamagui": "^1.132.9"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@faker-js/faker": "next",
    "@react-native-community/cli": "19.0.0",
    "@tamagui/vite-plugin": "^1.132.9",
    "@types/react": "^18.3.11",
    "tsx": "^4.19.0",
    "typescript": "^5.7.3",
    "vite": "^7.0.1"
  }
}
