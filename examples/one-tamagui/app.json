{
  "expo": {
    "name": "one-example",
    "slug": "one-example",
    "newArchEnabled": true,
    "platforms": [
      "ios",
      "android"
    ],
    "plugins": [
      "vxrn/expo-plugin",
      [
        "expo-build-properties",
        {
          "ios": {
            "ccacheEnabled": true
          }
        }
      ]
    ],
    "icon": "./src/app-icon.png",
    "splash": {
      "image": "./public/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "bundleIdentifier": "com.natew.oneexample"
    },
    "android": {
      "package": "com.natew.oneexample"
    }
  }
}
