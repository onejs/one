{
  "name": "onechat",
  "version": "1.1.415",
  "private": true,
  "sideEffects": false,
  "files": [],
  "type": "module",
  "scripts": {
    "android": "expo run:android",
    "backend:clean": "docker compose down --volumes; rm /tmp/onechat_replica.db || true",
    "backend:run-clean": "yarn backend:clean; yarn backend",
    "backend": "yarn docker up",
    "build:docker:app": "RUN=1 tsx ./src/ci/buildChatAppDocker.ts",
    "build:icon": "tauri icon ./src/tauri/icons/icon.png",
    "build:tauri": "tauri build --debug",
    "build:web": "one build",
    "dev:android": "one run:android",
    "dev:clean": "one dev --clean",
    "dev:ios": "one run:ios",
    "dev:tauri": "tauri dev",
    "local-install": "bun run ./scripts/local-install.ts",
    "dev": "one dev",
    "docker": "docker compose --env-file .env",
    "ios": "expo run:ios",
    "migrate-and-serve": "yarn migrate && yarn serve",
    "migrate:auth": "npx @better-auth/cli migrate --config ./src/better-auth/auth.ts",
    "migrate": "tsx ./src/postgres/migrate.ts",
    "seed": "tsx ./src/postgres/seed.ts",
    "serve": "one serve",
    "sst:build-and-deploy:production": "yarn build:docker:app && yarn sst:deploy:production",
    "sst:deploy:development": "sst deploy --stage development",
    "sst:deploy:production": "sst deploy --stage production",
    "sst:login": "aws sso login --sso-session=tamagui",
    "sst:refresh:production": "sst refresh --stage production",
    "typecheck": "tsc --noEmit",
    "zero:build-schema": "zero-build-schema -p ./src/zero/schema-build.ts -o ./src/zero/zero-schema.json",
    "zero": "yarn zero:build-schema && zero-cache --schema-file ./src/zero/zero-schema.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.709.0",
    "@aws-sdk/lib-storage": "^3.709.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@docsearch/react": "^3.6.1",
    "@floating-ui/react": "0.26.24",
    "@rocicorp/zero": "^0.12.2025012501",
    "@shopify/react-native-skia": "^1.7.6",
    "@tamagui/animations-moti": "^1.123.2",
    "@tamagui/config": "^1.123.2",
    "@tamagui/helpers-icon": "^1.123.2",
    "@tamagui/linear-gradient": "^1.123.2",
    "@tamagui/lucide-icons": "^1.123.2",
    "@tamagui/proxy-tabbable": "^1.123.2",
    "@tamagui/themes": "^1.123.2",
    "@tamagui/toast": "^1.123.2",
    "@tanstack/react-table": "^8.20.6",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-deep-link": "^2.0.1",
    "@tauri-apps/plugin-fs": "^2.2.0",
    "@tauri-apps/plugin-shell": "^2",
    "@uiw/react-md-editor": "^4.0.5",
    "@vxrn/better-auth": "workspace:*",
    "@vxrn/color-scheme": "workspace:*",
    "@vxrn/emitter": "workspace:*",
    "better-auth": "1.0.23-beta.3",
    "burnt": "^0.12.2",
    "expo": "~52.0.17",
    "flexsearch": "^0.7.43",
    "one": "workspace:*",
    "pg": "^8.13.1",
    "react": "^19.0.0",
    "react-hotkeys-hook": "^4.6.1",
    "react-native": "^0.76.5",
    "react-native-reanimated": "~3.16.5",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "4.4.0",
    "react-native-svg": "15.8.0",
    "sst": "^3.6.18",
    "tamagui": "^1.123.2",
    "ts-deepmerge": "^7.0.2",
    "valtio": "^2.1.2",
    "virtua": "^0.39.2"
  },
  "devDependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@react-native-community/cli": "15.1.3",
    "@tamagui/vite-plugin": "^1.123.2",
    "@tauri-apps/cli": "^2",
    "@types/aws-lambda": "8.10.147",
    "@types/pg": "^8.11.10",
    "dotenv": "^16.4.7",
    "sst": "^3.6.18",
    "tsx": "^4.19.0",
    "typescript": "^5.7.3",
    "unicode-emoji": "^2.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
