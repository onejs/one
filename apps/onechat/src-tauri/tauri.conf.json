{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "asdasd",
  "version": "0.1.0",
  "identifier": "com.asdasd.app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1421",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "asdasd",
        "width": 980,
        "hiddenTitle": true,
        "titleBarStyle": "Overlay",
        "transparent": true,
        "height": 700
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
