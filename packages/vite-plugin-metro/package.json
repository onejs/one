{
  "name": "@vxrn/vite-plugin-metro",
  "version": "1.1.504",
  "sideEffects": false,
  "type": "module",
  "main": "dist/cjs",
  "module": "dist/esm",
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs"
    },
    "./babel-transformer": {
      "types": "./types/transformer/babel-transformer.d.ts",
      "import": "./dist/esm/transformer/babel-transformer.mjs",
      "require": "./dist/cjs/transformer/babel-transformer.cjs"
    },
    "./empty": {
      "import": "./empty.js",
      "require": "./empty.js"
    }
  },
  "files": [
    "src",
    "types",
    "empty.js",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "yarn build --watch",
    "check": "yarn depcheck",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "peerDependencies": {
    "@expo/metro-config": "^0.19.8",
    "expo": "^53.0.4",
    "metro": "^0.82",
    "react-native": "~0.79.2",
    "vite": "^6.1.0"
  },
  "dependencies": {
    "@babel/core": "^7.26.8",
    "@vxrn/debug": "workspace:*",
    "babel-preset-expo": "*",
    "launch-editor": "^2.10.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@expo/metro-config": "^0.19.8",
    "@react-native/dev-middleware": "~0.79.2",
    "@tamagui/build": "^1.130.6",
    "@types/node": "^22.1.0",
    "depcheck": "^1.4.7",
    "expo": "^53.0.4",
    "metro": "^0.82",
    "vite": "^7.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
