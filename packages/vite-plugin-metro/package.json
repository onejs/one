{
  "name": "@vxrn/vite-plugin-metro",
  "version": "1.1.486",
  "sideEffects": false,
  "type": "module",
  "main": "dist/cjs",
  "module": "dist/esm",
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./babel-transformer": {
      "types": "./types/transformer/babel-transformer.d.ts",
      "import": "./dist/esm/transformer/babel-transformer.mjs",
      "require": "./dist/cjs/transformer/babel-transformer.cjs"
    }
  },
  "files": [
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "yarn build --watch",
    "check": "yarn depcheck",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "peerDependencies": {
    "@expo/metro-config": "^0.19.8",
    "expo": "^53.0.4",
    "metro": "^0.82",
    "react-native": "~0.79.2",
    "vite": "^6.1.0"
  },
  "dependencies": {
    "@babel/core": "^7.26.8",
    "@vxrn/debug": "workspace:*",
    "launch-editor": "^2.10.0",
    "picocolors": "^1.0.0",
    "tinyglobby": "^0.2.14"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@expo/metro-config": "^0.19.8",
    "@tamagui/build": "^1.126.4",
    "@types/node": "^22.1.0",
    "depcheck": "^1.4.7",
    "expo": "^53.0.4",
    "metro": "^0.82",
    "vite": "^6.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
