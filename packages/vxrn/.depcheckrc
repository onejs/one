ignorePatterns:
  - "dist"
ignores:
  - "@biomejs/biome"
  - "vxrn"
  - "@expo/cli"
  - "react"
  - "expo"
  # should probably be a peer dependency, this is only used while running prebuild without expo
  - "@react-native-community/template"
  # used dynamically via resolvePath() for vite aliases
  - "@vxrn/safe-area"
  - "@vxrn/query-string"
  - "@vxrn/url-parse"
  - "@vxrn/vendor"
