    "fast-glob": "^3.2.11",
    "find-node-modules": "^2.1.3",
    "fs-extra": "^11.2.0",
    "get-port-please": "^3.1.2",
    "glob": "^10.3.12",
    "h3": "^1.11.1",
    "h3-compression": "^0.3.2",
    "h3-proxy": "^1.13.0",
    "hono": "^4.3.10",
    "import-meta-resolve": "^4.0.0",
    "jiti": "^1.21.0",
    "metro-inspector-proxy": "^0.71.0",
    "mime": "^3.0.0",
