    "@vxrn/safe-area": "0.1.76",
    "@vxrn/vite-flow": "0.1.76",
    "@vxrn/vite-native-client": "0.1.76",
    "@vxrn/vite-native-hmr": "0.1.76",
    "@vxrn/vite-native-swc": "0.1.76",
    "citty": "^0.1.6",
    "crossws": "^0.2.4",
    "es-module-lexer": "^1.3.0",
