    "mdx-bundler": "^10.0.2",
    "parse-numeric-range": "^1.3.0",
    "reading-time": "1.3.0",
    "refractor": "^4.7.0",
    "rehype": "^12.0.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-parse": "^8.0.4",
    "rehype-slug": "^5.0.1",
    "shiki": "1.3.0",
    "unified": "^10.1.2",
    "unist-util-visit": "^2.0.3"
