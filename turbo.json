{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build:js": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": [
        "tsconfig.json",
        "biome.json",
        "package.json",
        "src/**/*",
        "tamagui-build.js",
        "externalNodePlugin.js"
      ],
      "outputs": [".next/**/*", "dist/**/*", "types/**/*"]
    },
    "check": {
      "cache": false,
      "outputs": []
    },
    "typecheck": {
      "cache": false,
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build:js"],
      "outputs": []
    },
    "test-macos": {
      "cache": false,
      "outputs": []
    },
    "test-ios": {
      "cache": false,
      "outputs": [],
      "passThroughEnv": ["IOS_TEST_CONTAINER_PATH_DEV", "IOS_TEST_CONTAINER_PATH_PROD", "DEV_PORT", "SIMULATOR_UDID"]
    },
    "test-ios:dev": {
      "cache": false,
      "outputs": [],
      "passThroughEnv": ["IOS_TEST_CONTAINER_PATH_DEV", "IOS_TEST_CONTAINER_PATH_PROD", "DEV_PORT", "SIMULATOR_UDID"]
    },
    "test-ios:prod": {
      "cache": false,
      "outputs": [],
      "passThroughEnv": ["IOS_TEST_CONTAINER_PATH_DEV", "IOS_TEST_CONTAINER_PATH_PROD", "DEV_PORT", "SIMULATOR_UDID"]
    },
    "clean": {
      "outputs": ["dist/**/*"],
      "cache": false
    },
    "clean:build": {
      "outputs": [],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "cache": false,
      "outputs": ["src/**/*"]
    }
  }
}
