{
  "name": "test-app-cases",
  "version": "1.9.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "one dev",
    "build:web": "one build",
    "serve": "one serve",
    "clean": "one clean",
    "test": "bun run test:dev && bun run test:prod",
    "test:dev": "TEST_ONLY=dev bun run test:all",
    "test:prod": "TEST_ONLY=prod bun run test:all",
    "test:all": "bun run test:basic && bun run test:not-found-at-root-with-assets && bun run test:dynamic-route-at-root-with-assets && bun run test:catch-all-route-at-root-with-assets && bun run test:spa-shell",
    "test:basic": "ONE_ROUTER_ROOT=app-cases/basic/app bun run vitest --run --reporter=dot --color=false",
    "test:catch-all-route-at-root-with-assets": "ONE_ROUTER_ROOT=app-cases/catch-all-route-at-root-with-assets/app bun run vitest --run --reporter=dot --color=false",
    "test:dynamic-route-at-root-with-assets": "ONE_ROUTER_ROOT=app-cases/dynamic-route-at-root-with-assets/app bun run vitest --run --reporter=dot --color=false",
    "test:not-found-at-root-with-assets": "ONE_ROUTER_ROOT=app-cases/not-found-at-root-with-assets/app bun run vitest --run --reporter=dot --color=false",
    "test:spa-shell": "ONE_ROUTER_ROOT=app-cases/spa-shell/app bun run vitest --run --reporter=dot --color=false",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@react-navigation/native": "~7.1.19",
    "@tamagui/config": "2.0.0-rc.14",
    "@vxrn/color-scheme": "workspace:*",
    "one": "workspace:*",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.2",
    "tamagui": "2.0.0-rc.14"
  },
  "devDependencies": {
    "playwright": "^1.57.0",
    "typescript": "^5.7.3",
    "vite": "^7.1.12",
    "vitest": "^4.0.6"
  }
}
