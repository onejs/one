    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.3",
    "react-native-web": "^0.21.2",
    "tamagui": "2.0.0-rc.14"
  },
  "devDependencies": {
    "@react-native-community/cli": "^20.0.2",
    "get-port-please": "^3.1.2",
    "playwright": "^1.57.0",
