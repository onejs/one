{
  "name": "test-routing-flicker",
  "version": "1.2.33",
  "private": true,
  "type": "module",
  "scripts": {
    "build:web": "one build",
    "clean": "one clean",
    "dev": "one dev",
    "dev:clean": "yarn dev --clean",
    "prod": "one build && one serve",
    "serve": "one serve",
    "test": "yarn test:spa && yarn test:ssg && yarn test:ssr",
    "test:spa": "DEFAULT_RENDER_MODE=spa TEST_ONLY=prod yarn vitest --run --reporter=verbose --color=false",
    "test:ssg": "DEFAULT_RENDER_MODE=ssg TEST_ONLY=prod yarn vitest --run --reporter=verbose --color=false",
    "test:ssr": "DEFAULT_RENDER_MODE=ssr TEST_ONLY=prod yarn vitest --run --reporter=verbose --color=false",
    "test:all": "yarn test:spa && yarn test:ssg",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@react-navigation/native": "~7.1.19",
    "@tamagui/config": "^1.140.4",
    "@vxrn/color-scheme": "workspace:*",
    "expo": "54.0.22",
    "one": "workspace:*",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.3",
    "react-native-web": "^0.21.2",
    "tamagui": "^1.140.4"
  },
  "devDependencies": {
    "@react-native-community/cli": "^20.0.2",
    "get-port-please": "^3.1.2",
    "playwright": "^1.49.1",
    "tsx": "^4.20.6",
    "typescript": "^5.7.3",
    "vite": "^7.1.12",
    "vitest": "^4.0.6"
  }
}
